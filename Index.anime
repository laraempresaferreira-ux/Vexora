  <script>
    // Default configuration
    const defaultConfig = {
      hero_title: "Exclusive 4K Anime Wallpapers for PC",
      hero_subtitle: "Transform your desktop with ultra-sharp, high-quality anime backgrounds designed specifically for large PC screens.",
      cta_button_text: "Buy Now — Instant Access",
